{{ form_errors(form) }}

<div class="ui stackable grid">
    <div class="three wide column">
        {{ form_row(form.publishable) }}
        {{ form_row(form.publishStartDate) }}
        {{ form_row(form.publishEndDate) }}
    </div>
    <div class="thirteen wide column">
        {{ form_row(form.name) }}
        {{ form_row(form.title) }}
        {{ form_row(form.body) }}
    </div>
</div>
