{% set main_event = _context.main_event ~ '.content' %}
{% set fallback_event = _context.fallback_event ~ '.content' %}

<div class="page-wrapper">
    {{ sylius_template_event(
        [main_event, fallback_event],
        _context|merge({
            main_event: main_event,
            fallback_event: fallback_event,
        })
    ) }}
</div>
