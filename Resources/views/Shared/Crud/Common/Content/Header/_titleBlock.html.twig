{% set main_event = _context.main_event ~ '.title_block' %}
{% set fallback_event = _context.fallback_event ~ '.title_block' %}

<div class="row g-2 mt-2">
    {{ twig_event(
        [main_event, fallback_event],
        _context|merge({
            main_event: main_event,
            fallback_event: fallback_event,
        })
    ) }}
</div>
