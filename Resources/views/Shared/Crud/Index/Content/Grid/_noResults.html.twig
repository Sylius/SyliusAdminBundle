{% set event_name = event_prefix ~ '.no_results' %}
{% set resource_event_name = resource_event_prefix ~ '.no_results' %}

{% if resources.data|length == 0 %}
    <div class="card">
        <div class="empty">
            {{ sylius_template_event([resource_event_name, event_name], _context|merge({ event_prefix: event_name, resource_event_prefix: resource_event_name })) }}
        </div>
    </div>
{% endif %}
