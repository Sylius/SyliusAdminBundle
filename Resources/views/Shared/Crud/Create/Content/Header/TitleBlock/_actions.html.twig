{% import '@SyliusAdmin/Shared/Helper/button.html.twig' as _button %}

{% set main_event = twig_event_name(_context.main_event, 'actions') %}
{% set fallback_event = twig_event_name(_context.fallback_event, 'actions') %}

<div class="col-12 col-md-auto ms-auto d-print-none">
    <div class="btn-list">
        {{ twig_event([main_event, fallback_event], _context|merge({ main_event: main_event, fallback_event: fallback_event })) }}
    </div>
</div>
